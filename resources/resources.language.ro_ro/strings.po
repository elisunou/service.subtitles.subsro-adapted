msgctxt "#30001"
msgid "Authentication"
msgstr "Autentificare"

msgctxt "#30002"
msgid "Subs.ro API Key"
msgstr "Cheie API Subs.ro"

msgctxt "#30003"
msgid "How to obtain an API key?"
msgstr "Cum obțin o cheie API?"

msgctxt "#30004"
msgid "1. Visit https://subs.ro/api"
msgstr "1. Accesează https://subs.ro/api"

msgctxt "#30005"
msgid "2. Login with your account"
msgstr "2. Autentifică-te cu contul tău"

msgctxt "#30006"
msgid "3. Generate/Copy API key"
msgstr "3. Generează/Copiază cheia API"

msgctxt "#30007"
msgid "4. Paste the key above"
msgstr "4. Lipește cheia mai sus"

msgctxt "#30008"
msgid "Check API quota on every search"
msgstr "Verifică quota API la fiecare căutare"

msgctxt "#30009"
msgid "Show warning when quota drops below 10%"
msgstr "Afișează avertisment când quota scade sub 10%"

